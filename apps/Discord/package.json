{
  "name": "@lentix/discord",
  "version": "0.0.0",
  "description": "Discord Apps built in typescript",
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "watch": "tsc -w",
    "start": "ts-node src/index.ts",
    "start:tsc": "tsc && node dist/index.js",
    "build": "tsc",
    "reg-cmds": "node dist/src/deploy-commands.js",
    "delete-cmds": "node dist/src/delete-commands.js",
    "dev": "tsc && nodemon dist/index.js"
  },
  "author": "James Pryor",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": ""
  },
  "dependencies": {
    "@discordjs/builders": "1.4.0",
    "@discordjs/rest": "1.5.0",
    "@discordjs/voice": "^0.14.0",
    "@sentry/integrations": "^7.31.1",
    "@sentry/node": "^7.31.1",
    "@types/node": "^18.8.4",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.48.2",
    "discord-api-types": "^0.36.3",
    "discord.js": "^14.7.1",
    "discord.js-commando": "^0.12.3",
    "discord.js-rate-limiter": "^1.3.2",
    "dotenv": "^10.0.0",
    "eslint": "^8.25.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.3.3",
    "mongoose": "^6.8.4",
    "node-fetch": "^3.2.10",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "winston": "^3.8.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=9.2.0",
    "pnpm": ">=7.25.0"
  },
  "lint-staged": {
    "**/*.{js,ts}": "eslint --ext .js,.ts",
    "**/*.{js,ts,json,md}": "prettier --ignore-path .gitignore --write"
  },
  "eslintConfig": {
    "extends": "./config/.eslintrc.js"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "packageManager": "pnpm@7.25.0"
}
